{
  "private": true,
  "scripts": {
    "clean-build": "npm run clean-install;npm run build",
    "clean-build-all": "npm run clean-install;(cd ui;npm run clean-build);npm run build",
    "clean-install": "rm -rf node_modules/ .cache/ package-lock.json;npm i --cache=.cache/npm",
    "build": "node_modules/.bin/antora antora-playbook.yml --stacktrace --fetch --clean",
    "build-ui": "(cd ui;gulp)",
    "build-all": "npm run build-ui;npm run build"
  },
  "devDependencies": {
    "@antora/cli": "^3.0.0-alpha.1",
    "@antora/site-generator-default": "^3.0.0-alpha.1",
    "@djencks/asciidoctor-tabset-block": "https://experimental-repo.s3-us-west-1.amazonaws.com/djencks-asciidoctor-tabset-block-v0.0.1.tgz"
  }
}
